.paper
  padding: 20px 40px 30px 40px
.wrapper
  max-width: 1200px
  padding: 20px 40px 30px 40px
  margin: 0 auto
.gridWrapper
  display: grid
  grid-template-columns: 50% 50%
  grid-gap: 20px
  max-width: 1200px
  width: 100%

.a
  composes: paper
  grid-column: 1 / 2
  grid-row: 1 / 3
.b
  composes: paper
  grid-column: 2 / 3
  grid-row: 1 / 2
.c
  composes: paper
  grid-column: 2 / 3
  grid-row: 2 / 3
